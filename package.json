{
  "name": "workwork",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startdev": "env-cmd -e dev tsup --watch --onSuccess \"npm run copy-assets-and-run\"",
    "startprod": "env-cmd -e prod nodemon build/app.mjs",
    "build": "tsc --noEmit && tsup && npm run copy:assets",
    "build:notypecheck": "tsup",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "copy:assets": "copy-assets src/views build/views --ext=.ejs",
    "copy-assets-and-run": "concurrently \"npm:copy:assets\" \"node build/app.js\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^11.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-bearer-token": "^2.4.0",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^6.1.0",
    "express-session": "^1.17.2",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@swc/core": "^1.2.135",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "concurrently": "^7.0.0",
    "copy-assets": "^1.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.5.0",
    "nodemon": "^2.0.15",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4"
  }
}
